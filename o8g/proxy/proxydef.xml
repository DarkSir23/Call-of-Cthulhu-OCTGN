<template src="proxy/template/neutral_character.png" default="False"> 
    <matches> 
      <match name="Type" value="Character" /> 
    </matches> 
	<overlayblocks>
			<conditional>
			<if property="Faction" value="Hastur">
				<link block="character_overlay_hastur" />
				<link block="symbol_hastur" />
			</if>
		</conditional>
			<conditional>
			<if property="Faction" value="Miskatonic University">
				<link block="character_overlay_miskatonic" />
				<link block="symbol_miskatonic" />
			</if>
		</conditional>
			<conditional>
			<if property="Faction" value="Shub-Niggurath">
				<link block="character_overlay_shub" />
				<link block="symbol_shub" />
			</if>
		</conditional>
			<conditional>
			<if property="Faction" value="The Agency">
				<link block="character_overlay_agency" />
				<link block="symbol_agency" />
			</if>
		</conditional>
			<conditional>
			<if property="Faction" value="Cthulhu">
				<link block="character_overlay_cthulhu" />
				<link block="symbol_cthulhu" />
			</if>
		</conditional>
			<conditional>
			<if property="Faction" value="The Order of the Silver Twilight">
				<link block="character_overlay_silver_twilight" />
				<link block="symbol_silver_twilight" />
			</if>
		</conditional>
			<conditional>
			<if property="Faction" value="The Syndicate">
				<link block="character_overlay_syndicate" />
				<link block="symbol_syndicate" />
			</if>
		</conditional>
			<conditional>
			<if property="Faction" value="Yog-Sothoth">
				<link block="character_overlay_yog" />
				<link block="symbol_yog" />
			</if>
		</conditional>
		<conditional>
			<if property="Resource Icon" value="__">
				<link block="resource_transient_agency" />
			</if>
		</conditional>
		<conditional>
			<if property="Resource Icon" value="{{">
				<link block="resource_transient_miskatonic" />
			</if>
		</conditional>
		<conditional>
			<if property="Resource Icon" value="??">
				<link block="resource_transient_neutral" />
			</if>
		</conditional>
		<conditional>
			<if property="Resource Icon" value="&lt;&lt;">
				<link block="resource_transient_yog" />
			</if>
		</conditional>
		<conditional>
			<if property="Resource Icon" value="Z">
				<link block="resource_zoog" />
			</if>
		</conditional>
		<link block="character_overlay_image" />
	</overlayblocks>
	<textblocks> 
		<conditional>
			<if property="Keyword" value="">
				<link block="card_text">
					<property name="Card Text" />
				</link>
			</if>
			<else>
				<link block="card_keyword">
					<property name="Keyword" />
				</link>
				<link block="card_text_with_keyword">
					<property name="Card Text" />
				</link>
			</else>
		</conditional>
		  <link block="card_name">
			<property name="Name"  />
		  </link>
		  <link block="card_descriptor">
			<property name="Descriptor" />
		</link>
		  <link block="card_cost">
			<property name="Cost" />
		  </link>
		  <link block="card_skill">
			<property name="Skill" />
		  </link>
		  <link block="card_subtypes">
			<property name="Subtypes" />
		</link>
		<link block="card_icons">
			<property name="Icons" />
		</link>
		<link block="struggle_icons">
			<property name="Struggle Icons" />
		</link>
	</textblocks> 
</template>
